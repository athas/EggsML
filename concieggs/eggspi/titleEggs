#!/bin/sh

hour="$(dateSecs $1 +%H)"
minute="$(dateSecs $1 +%M)"

if [ $hour -eq 13 -a $minute -eq 0 ]; then
    if christmas; do
        echo "juleman Eggs $(timeToRelDay $1)"
    else
        echo "gentleman Eggs $(timeToRelDay $1)"
    fi
elif [ $hour -eq 14 -a $minute -eq 0 ]; then
    echo "Eggs royale $(timeToRelDay $1)"
else
    echo "Eggs $(timeToDate $1)"
fi
