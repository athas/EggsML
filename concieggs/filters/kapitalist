# VÃ¦r kapitalist

import sys
import re

def flatten(xss):
    return [ x for xs in xss for x in xs ]

upper = "ğ”¸ğ”¹â„‚ğ”»ğ”¼ğ”½ğ”¾â„ğ•€ğ•ğ•‚ğ•ƒğ•„â„•ğ•†â„™â„šâ„ğ•Šğ•‹ğ•Œğ•ğ•ğ•ğ•â„¤"
lower = "ğ•’ğ•“ğ•”ğ••ğ•–ğ•—ğ•˜ğ•™ğ•šğ•›ğ•œğ•ğ•ğ•Ÿğ• ğ•¡ğ•¢ğ•£ğ•¤ğ•¥ğ•¦ğ•§ğ•¨ğ•©ğ•ªğ•«"
table = dict(flatten([ [ (ord('a') + i, lower[i]),
                         (ord('A') + i, upper[i]) ]
                       for i in range(26) ]))

a = table[ord('a')]
e = table[ord('e')]
o = table[ord('o')]

table[ord('Ã¦')] = a + e
table[ord('Ã¸')] = o
table[ord('Ã¥')] = a + a

A = table[ord('A')]
E = table[ord('E')]
O = table[ord('O')]

table[ord('Ã†')] = A + E
table[ord('Ã˜')] = O
table[ord('Ã…')] = A + A

if __name__ == "__main__":
    for line in sys.stdin:
        print(line.translate(table))
